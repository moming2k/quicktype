{
  "name": "quicktype",
  "version": "14.0.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/quicktype/quicktype",
  "scripts": {
    "pub": "script/publish.sh",
    "prepare": "npm run build",
    "preinstall": "(cd src/core ; npm install) && (cd src/typescript-input ; npm install)",
    "build": "script/build.ts",
    "test": "script/test",
    "start": "script/watch",
    "pkg": "script/make-pkgs.sh",
    "tslint": "(cd src/core ; npm run tslint) && (cd src/typescript-input ; npm run tslint) && tslint --project src --exclude 'src/__tests__/**'",
    "clean": "rm -rf dist node_modules *~ && (cd src/core ; npm run clean) && (cd src/typescript-input ; npm run clean)"
  },
  "dependencies": {
    "quicktype-core": "file:src/core",
    "quicktype-typescript-input": "file:src/typescript-input",
    "chalk": "^2.4.1",
    "command-line-args": "^4.0.6",
    "command-line-usage": "^4.1.0",
    "immutable": "^4.0.0-rc.9",
    "is-url": "^1.2.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "node-fetch": "^1.7.1",
    "node-persist": "^2.1.0",
    "universal-analytics": "^0.4.16",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/is-url": "^1.2.28",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.10.10",
    "@types/node-persist": "0.0.33",
    "@types/shelljs": "^0.7.8",
    "@types/universal-analytics": "^0.4.2",
    "ajv": "^5.5.2",
    "compare-versions": "^3.1.0",
    "deep-equal": "^1.0.1",
    "elm": "^0.18.0",
    "exit": "^0.1.2",
    "flow-bin": "^0.66.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^22.4.3",
    "pkg": "^4.3.1",
    "promise-timeout": "^1.3.0",
    "shelljs": "^0.7.8",
    "ts-jest": "^22.4.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "typescript": "~2.8.3",
    "uglify-js": "^3.3.22",
    "watch": "^1.0.2"
  },
  "files": [
    "dist/**"
  ],
  "bin": "dist/cli/index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "src/tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/.*)\\.test\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
